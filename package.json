{
  "name": "nodecft",
  "version": "0.0.5",
  "keywords": [
    "CFT with NodeJS",
    "NodeJS CTF",
    "CTF",
    "AWS",
    "infrastructure as code software tool"
  ],
  "description": "NodeCFT - Simple infrastructure as code software tool on NodeJS",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/priyanthag/NodeCFT.git"
  },
  "main": "bin/nodecft.js",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "check": "npm run lint",
    "build": "babel src --out-dir bin"
  },
  "pre-commit": [
    "lint:fix",
    "check"
  ],
  "author": "Priyantha Gunawardena <pgdesigning@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "child_process": "^1.0.2",
    "util": "^0.11.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "pre-commit": "^1.2.2",
    "standard": "^12.0.1"
  },
  "standard": {
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect",
      "test",
      "gContext",
      "jest",
      "__rewire_reset_all__"
    ],
    "parser": "babel-eslint",
    "plugins": [],
    "ignore": [
      "src/**/__tests__/**/*",
      "src/**/__tests__/*",
      "src/fixtures/*",
      "bin/*"
    ]
  }
}
